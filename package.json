{
  "scripts": {
    "start": "yarn install && yarn webpack serve --mode development",
    "test": "yarn jest",
    "lint": "yarn eslint .",
    "checkEveryChangeAreCommit": "git diff --quiet --exit-code",
    "pushDocsFolder": "git add ./docs && git commit -m 'update github page' && git push",
